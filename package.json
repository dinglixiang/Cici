{
  "name": "cici",
  "version": "0.0.11",
  "description": "Yet another static website generator built on top of Vue 2 and Webpack, with a nice comment system",
  "bin": "./bin/cici.js",
  "scripts": {
    "bundle": "cross-env NODE_ENV=production webpack -p --progress --hide-modules",
    "transpile": "./node_modules/.bin/babel src/utils --out-dir lib/utils && ./node_modules/.bin/babel src/builder --out-dir lib/builder ",
    "prestart": "npm run bundle && npm run transpile",
    "prepublish": "npm run bundle && npm run transpile",
    "start": "./bin/cici.js && http-server public",
    "pretest": "npm run transpile",
    "test": "mocha --ui bdd --compilers js:babel-core/register --require babel-polyfill --recursive --reporter spec --check-leaks -t 10000 test"
  },
  "engines": {
    "node": ">=4",
    "npm": ">=3"
  },
  "directories": {
    "lib": "lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/metrue/metrue.github.io.git"
  },
  "keywords": [
    "blog",
    "github",
    "vuejs",
    "webpack"
  ],
  "author": "metrue (https://github.com/metrue)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/metrue/metrue.github.io/issues"
  },
  "homepage": "https://github.com/metrue/metrue.github.io#readme",
  "devDependencies": {
    "chai": "^3.5.0",
    "cross-env": "^3.1.0",
    "css-loader": "^0.25.0",
    "eslint": "^3.7.0",
    "eslint-config-standard": "^6.0.0",
    "eslint-loader": "^1.5.0",
    "eslint-plugin-html": "^1.5.3",
    "eslint-plugin-promise": "^3.3.0",
    "eslint-plugin-standard": "^2.0.0",
    "extract-text-webpack-plugin": "^2.0.0-beta.4",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.22.0",
    "inject-loader": "^3.0.0-beta2",
    "json-loader": "^0.5.4",
    "mocha": "^3.1.2",
    "rimraf": "^2.5.4",
    "sinon": "^1.17.6",
    "sinon-chai": "^2.8.0",
    "style-loader": "^0.13.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^2.3.1",
    "vue-hot-reload-api": "^2.0.0",
    "vue-html-loader": "^1.2.3",
    "vue-loader": "^10.0.0",
    "vue-style-loader": "^1.0.0",
    "vue-template-compiler": "^2.1.0",
    "webpack": "2.1.0-beta.27",
    "webpack-dev-server": "2.1.0-beta.11"
  },
  "dependencies": {
    "babel-polyfill": "^6.7.4",
    "babel-cli": "^6.22.2",
    "babel-core": "^6.0.0",
    "babel-helper-vue-jsx-merge-props": "^2.0.2",
    "babel-loader": "^6.0.0",
    "babel-eslint": "^6.0.4",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-plugin-transform-vue-jsx": "^3.3.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-stage-1": "^6.13.0",
    "commander": "^2.9.0",
    "ejs": "^2.5.5",
    "es6-promise": "^4.0.0",
    "front-matter": "^2.1.0",
    "fs-extra": "^2.0.0",
    "isomorphic-fetch": "^2.2.1",
    "marked": "^0.3.6",
    "mkdirp": "^0.5.1",
    "normalize.css": "^5.0.0",
    "prismjs": "^1.5.1",
    "timeago.js": "^2.0.2",
    "vue": "^2.1.0",
    "vue-router": "^2.0.0",
    "vue-server-renderer": "^2.1.10"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ],
    "plugins": [
      "transform-regenerator"
    ]
  }
}
